{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "jwtAccessToken": {
      "type": "string"
    },
    "jwtRefreshToken": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "countryCode": {
          "type": "string"
        },
        "created": {
          "type": "integer"
        },
        "isPhoneVerified": {
          "type": "boolean"
        },
        "isEmailVerified": {
          "type": "boolean"
        },
        "wallets": {
          "type": "array",
          "items": {}
        },
        "contacts": {
          "type": "array",
          "items": {}
        },
        "files": {
          "type": "array",
          "items": {}
        },
        "nftCollections": {
          "type": "array",
          "items": {}
        }
      },
      "required": [
        "userId",
        "firstName",
        "lastName",
        "status",
        "email",
        "phone",
        "countryCode",
        "created"
      ]
    }
  },
  "required": [
    "jwtAccessToken",
    "jwtRefreshToken",
    "user"
  ]
}